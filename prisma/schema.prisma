generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model perfil {
  id Int @id @default(autoincrement())
  perfil_nome String?
  usuario usuario?
  @@map("perfil")
}
model usuario{
  id Int @id @default(autoincrement())
  nome String
  email String @unique
  senha String 
  id_perfil Int @unique
  perfil perfil? @relation(fields: [id_perfil], references: [id])
  @@map("usuario")
}
